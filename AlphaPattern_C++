#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
         if( i == 0 || i == n-1 || j==n/2 ) 
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}





================================================================================================================================
///////////////////////////////////////////////            J             ///////////////////////////////////////////////////////


#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
          if( i == 0 || (i == n-1 && j<n/2) || j==n/2 || (j==0 && i>=n/2) ) 
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}




================================================================================================================================
///////////////////////////////////////////////            K             ///////////////////////////////////////////////////////



#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
          if( j == 0 || (i == n/2 && j<n/2) || (i+j==n-1 && j>=n/2) || (i==j && i>=n/2)) 
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}


================================================================================================================================
///////////////////////////////////////////////            L             ///////////////////////////////////////////////////////



#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
         if( j == 0 || i == n-1)
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}



================================================================================================================================
///////////////////////////////////////////////             M             ///////////////////////////////////////////////////////



#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
         if( j == 0 || j == n-1 || (i==j && j<=n/2) || (i+j==n-1 && j>n/2)) 
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}




=================================================================================================================================
///////////////////////////////////////////////             N             ///////////////////////////////////////////////////////



#include <iostream>
using namespace std;

int main ()
{
  int n = 5;
  for (int i=0; i<n ; i++)
  {
      for(int j=0;j<n ; j++)
      {
       if( j == 0 || j == n-1 || i==j ) 
          {
             cout<<"* ";  
          }
          else
          {
              cout<<"  ";
          }
               }
      cout<<endl;
  }
  return 0;
}
